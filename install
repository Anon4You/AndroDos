apt update -y
command -v python > /dev/null 2>&1 || { echo >&2 "python not found !, Installing... python"; pkg install python -y 1; }
curl -LO https://raw.githubusercontent.com/Anon4You/AndroDos/main/androdos
mv androdos $PREFIX/bin
chmod +x $PREFIX/bin/androdos
clear
echo "now type 'androdos' from any directory to run this tool"
